<?xml version="1.0"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">
  
  <!-- Java.ProducerCodeHandler.Account -->
  <xsd:element name="AccountProducerCodeHandler">
    <xsd:complexType>
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element ref="ProducerStatus" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="SystemPermType" minOccurs="0" maxOccurs="unbounded"/>
      </xsd:choice>
      <xsd:attribute name="desc" type="xsd:string"/>
      <xsd:attribute name="noPermissionDisplayKey" type="xsd:string"/>
      <!-- The permission key -->
      <xsd:attribute name="permKey" type="xsd:NMTOKEN" use="required"/>
    </xsd:complexType>
  </xsd:element>
  
  <xsd:element name="ContactCreatePermission">
    <xsd:complexType>
      <xsd:choice minOccurs="0" maxOccurs="unbounded"/>
      <xsd:attribute name="permission" type="SystemPerm" use="required"/>
    </xsd:complexType>
  </xsd:element>
  
  <xsd:element name="ContactDeletePermission">
    <xsd:complexType>
      <xsd:choice minOccurs="0" maxOccurs="unbounded"/>
      <xsd:attribute name="permission" type="SystemPerm" use="required"/>
    </xsd:complexType>
  </xsd:element>
  
  <xsd:element name="ContactEditPermission">
    <xsd:complexType>
      <xsd:choice minOccurs="0" maxOccurs="unbounded"/>
      <xsd:attribute name="permission" type="SystemPerm" use="required"/>
    </xsd:complexType>
  </xsd:element>
  
  <xsd:simpleType name="ContactEntity">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="Adjudicator"/>
      <xsd:enumeration value="Company"/>
      <xsd:enumeration value="CompanyVendor"/>
      <xsd:enumeration value="Contact"/>
      <xsd:enumeration value="LegalVenue"/>
      <xsd:enumeration value="Person"/>
      <xsd:enumeration value="PersonVendor"/>
      <xsd:enumeration value="Place"/>
      <xsd:enumeration value="UserContact"/>
    </xsd:restriction>
  </xsd:simpleType>
  
  <!-- Contact-related permissions -->
  <xsd:element name="ContactPermissions">
    <xsd:complexType>
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element ref="ContactSubtypeAccessProfile" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="ContactTagAccessProfile" minOccurs="0" maxOccurs="unbounded"/>
      </xsd:choice>
    </xsd:complexType>
  </xsd:element>
  
  <xsd:element name="ContactSubtypeAccessProfile">
    <xsd:complexType>
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element ref="ContactCreatePermission" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="ContactDeletePermission" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="ContactEditPermission" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="ContactViewPermission" minOccurs="0" maxOccurs="unbounded"/>
      </xsd:choice>
      <xsd:attribute name="entity" type="ContactEntity" use="required"/>
    </xsd:complexType>
  </xsd:element>
  
  <xsd:simpleType name="ContactTag">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="Claim Party"/>
      <!-- Client -->
      <xsd:enumeration value="Client"/>
      <!-- Vendor -->
      <xsd:enumeration value="Vendor"/>
    </xsd:restriction>
  </xsd:simpleType>
  
  <xsd:element name="ContactTagAccessProfile">
    <xsd:complexType>
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element ref="ContactCreatePermission" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="ContactDeletePermission" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="ContactEditPermission" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="ContactViewPermission" minOccurs="0" maxOccurs="unbounded"/>
      </xsd:choice>
      <xsd:attribute name="tag" type="ContactTag" use="required"/>
    </xsd:complexType>
  </xsd:element>
  
  <xsd:element name="ContactViewPermission">
    <xsd:complexType>
      <xsd:choice minOccurs="0" maxOccurs="unbounded"/>
      <xsd:attribute name="permission" type="SystemPerm" use="required"/>
    </xsd:complexType>
  </xsd:element>
  
  <xsd:element name="DocumentAccessProfile">
    <xsd:complexType>
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element ref="DocumentCreatePermission" minOccurs="0" maxOccurs="1"/>
        <xsd:element ref="DocumentDeletePermission" minOccurs="0" maxOccurs="1"/>
        <xsd:element ref="DocumentEditPermission" minOccurs="0" maxOccurs="1"/>
        <xsd:element ref="DocumentViewPermission" minOccurs="0" maxOccurs="1"/>
      </xsd:choice>
      <xsd:attribute name="securitylevel" type="DocumentSecurityLevel" use="required"/>
    </xsd:complexType>
  </xsd:element>
  
  <xsd:element name="DocumentCreatePermission">
    <xsd:complexType>
      <xsd:choice minOccurs="0" maxOccurs="unbounded"/>
      <xsd:attribute name="permission" type="SystemPerm" use="required"/>
    </xsd:complexType>
  </xsd:element>
  
  <xsd:element name="DocumentDeletePermission">
    <xsd:complexType>
      <xsd:choice minOccurs="0" maxOccurs="unbounded"/>
      <xsd:attribute name="permission" type="SystemPerm" use="required"/>
    </xsd:complexType>
  </xsd:element>
  
  <xsd:element name="DocumentEditPermission">
    <xsd:complexType>
      <xsd:choice minOccurs="0" maxOccurs="unbounded"/>
      <xsd:attribute name="permission" type="SystemPerm" use="required"/>
    </xsd:complexType>
  </xsd:element>
  
  <!-- Encapsulates the permission mappings for a given DocumentSecurityType value -->
  <xsd:element name="DocumentPermissions">
    <xsd:complexType>
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element ref="DocumentAccessProfile" minOccurs="0" maxOccurs="unbounded"/>
      </xsd:choice>
    </xsd:complexType>
  </xsd:element>
  
  <xsd:simpleType name="DocumentSecurityLevel">
    <xsd:restriction base="xsd:string">
      <!-- Document that should not be viewed by people outside the company -->
      <xsd:enumeration value="internalonly"/>
      <!-- Document that is sensitive in nature -->
      <xsd:enumeration value="sensitive"/>
      <!-- Document that does not require access restriction -->
      <xsd:enumeration value="unrestricted"/>
    </xsd:restriction>
  </xsd:simpleType>
  
  <xsd:element name="DocumentViewPermission">
    <xsd:complexType>
      <xsd:choice minOccurs="0" maxOccurs="unbounded"/>
      <xsd:attribute name="permission" type="SystemPerm" use="required"/>
    </xsd:complexType>
  </xsd:element>
  
  <xsd:simpleType name="Entity">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="Account"/>
      <xsd:enumeration value="Activity"/>
      <xsd:enumeration value="ActivityPattern"/>
      <xsd:enumeration value="Attribute"/>
      <xsd:enumeration value="AttributeUser"/>
      <xsd:enumeration value="Audit"/>
      <xsd:enumeration value="BusinessWeek"/>
      <xsd:enumeration value="Cancellation"/>
      <xsd:enumeration value="Contact"/>
      <xsd:enumeration value="Credential"/>
      <xsd:enumeration value="Document"/>
      <xsd:enumeration value="Group"/>
      <xsd:enumeration value="GroupRegion"/>
      <xsd:enumeration value="GroupUser"/>
      <xsd:enumeration value="Holiday"/>
      <xsd:enumeration value="Issuance"/>
      <xsd:enumeration value="Job"/>
      <xsd:enumeration value="Note"/>
      <xsd:enumeration value="Organization"/>
      <xsd:enumeration value="PolicyChange"/>
      <xsd:enumeration value="PolicyPeriod"/>
      <xsd:enumeration value="ProducerCode"/>
      <xsd:enumeration value="Region"/>
      <xsd:enumeration value="Reinstatement"/>
      <xsd:enumeration value="Renewal"/>
      <xsd:enumeration value="Rewrite"/>
      <xsd:enumeration value="RewriteNewAccount"/>
      <xsd:enumeration value="Role"/>
      <xsd:enumeration value="RolePrivilege"/>
      <xsd:enumeration value="ScriptParameter"/>
      <xsd:enumeration value="SecurityZone"/>
      <xsd:enumeration value="Submission"/>
      <xsd:enumeration value="User"/>
      <xsd:enumeration value="UserRole"/>
    </xsd:restriction>
  </xsd:simpleType>
  
  <!-- Java.ProducerCodeHandler.Job -->
  <xsd:element name="JobProducerCodeHandler">
    <xsd:complexType>
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element ref="ProducerStatus" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="SystemPermType" minOccurs="0" maxOccurs="unbounded"/>
      </xsd:choice>
      <xsd:attribute name="desc" type="xsd:string"/>
      <!-- Java.ProducerCodeHandler.JobType -->
      <xsd:attribute name="jobType" type="xsd:NMTOKEN" use="required"/>
      <xsd:attribute name="noPermissionDisplayKey" type="xsd:string"/>
      <!-- The permission key -->
      <xsd:attribute name="permKey" type="xsd:NMTOKEN" use="required"/>
    </xsd:complexType>
  </xsd:element>
  
  <!-- Encapsulates the permission mappings for a given NoteSecurityType value -->
  <xsd:element name="NoteAccessProfile">
    <xsd:complexType>
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element ref="NoteCreatePermission" minOccurs="0" maxOccurs="1"/>
        <xsd:element ref="NoteDeletePermission" minOccurs="0" maxOccurs="1"/>
        <xsd:element ref="NoteEditBodyPermission" minOccurs="0" maxOccurs="1"/>
        <xsd:element ref="NoteEditPermission" minOccurs="0" maxOccurs="1"/>
        <xsd:element ref="NoteViewPermission" minOccurs="0" maxOccurs="1"/>
      </xsd:choice>
      <xsd:attribute name="securitylevel" type="xsd:NMTOKEN" use="required"/>
    </xsd:complexType>
  </xsd:element>
  
  <xsd:element name="NoteCreatePermission">
    <xsd:complexType>
      <xsd:choice minOccurs="0" maxOccurs="unbounded"/>
      <xsd:attribute name="permission" type="SystemPerm" use="required"/>
    </xsd:complexType>
  </xsd:element>
  
  <xsd:element name="NoteDeletePermission">
    <xsd:complexType>
      <xsd:choice minOccurs="0" maxOccurs="unbounded"/>
      <xsd:attribute name="permission" type="SystemPerm" use="required"/>
    </xsd:complexType>
  </xsd:element>
  
  <xsd:element name="NoteEditBodyPermission">
    <xsd:complexType>
      <xsd:choice minOccurs="0" maxOccurs="unbounded"/>
      <xsd:attribute name="permission" type="SystemPerm" use="required"/>
    </xsd:complexType>
  </xsd:element>
  
  <xsd:element name="NoteEditPermission">
    <xsd:complexType>
      <xsd:choice minOccurs="0" maxOccurs="unbounded"/>
      <xsd:attribute name="permission" type="SystemPerm" use="required"/>
    </xsd:complexType>
  </xsd:element>
  
  <!-- Note-related permissions, allowing a mapping between actions such as view, edit, etc.
       and a required system permission based on the Note's SecurityType property -->
  <xsd:element name="NotePermissions">
    <xsd:complexType>
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element ref="NoteAccessProfile" minOccurs="0" maxOccurs="unbounded"/>
      </xsd:choice>
    </xsd:complexType>
  </xsd:element>
  
  <xsd:simpleType name="NoteSecurityLevel">
    <xsd:restriction base="xsd:string">
      <!-- Note viewable by internal users only -->
      <xsd:enumeration value="internalonly"/>
      <!-- Note which is sensitive in nature -->
      <xsd:enumeration value="sensitive"/>
      <!-- Note viewable by any user in the system -->
      <xsd:enumeration value="unrestricted"/>
    </xsd:restriction>
  </xsd:simpleType>
  
  <xsd:element name="NoteViewPermission">
    <xsd:complexType>
      <xsd:choice minOccurs="0" maxOccurs="unbounded"/>
      <xsd:attribute name="permission" type="SystemPerm" use="required"/>
    </xsd:complexType>
  </xsd:element>
  
  <!-- Java.ProducerCodeHandler.PolicyPeriod -->
  <xsd:element name="PolicyPeriodProducerCodeHandler">
    <xsd:complexType>
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element ref="ProducerStatus" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="SystemPermType" minOccurs="0" maxOccurs="unbounded"/>
      </xsd:choice>
      <xsd:attribute name="desc" type="xsd:string"/>
      <!-- Java.ProducerCodeHandler.IsAllowedForPCOfRecord -->
      <xsd:attribute name="isAllowedForPCOfRecord" type="xsd:boolean" default="false"/>
      <xsd:attribute name="noPermissionDisplayKey" type="xsd:string"/>
      <!-- The permission key -->
      <xsd:attribute name="permKey" type="xsd:NMTOKEN" use="required"/>
    </xsd:complexType>
  </xsd:element>
  
  <!-- Java.ProducerCodeHandler.Status -->
  <xsd:element name="ProducerStatus">
    <xsd:complexType>
      <xsd:choice minOccurs="0" maxOccurs="unbounded"/>
      <!-- Java.ProducerCodeHandler.Code -->
      <xsd:attribute name="code" type="xsd:NMTOKEN" use="required"/>
    </xsd:complexType>
  </xsd:element>
  
  <xsd:element name="SecurityConfig">
    <xsd:complexType>
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element ref="AccountProducerCodeHandler" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="ContactPermissions" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="DocumentPermissions" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="JobProducerCodeHandler" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="NotePermissions" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="PolicyPeriodProducerCodeHandler" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="StaticHandler" minOccurs="0" maxOccurs="unbounded"/>
        <xsd:element ref="WrapHandler" minOccurs="0" maxOccurs="unbounded"/>
      </xsd:choice>
      <xsd:attribute name="handlerClass" type="xsd:string"/>
      <xsd:attribute name="permissionClass" type="xsd:string"/>
    </xsd:complexType>
  </xsd:element>
  
  <!-- This will create a static handler for the given permission -->
  <xsd:element name="StaticHandler">
    <xsd:complexType>
      <xsd:choice minOccurs="0" maxOccurs="unbounded">
        <xsd:element ref="SystemPermType" minOccurs="0" maxOccurs="unbounded"/>
      </xsd:choice>
      <xsd:attribute name="desc" type="xsd:string"/>
      <!-- The entity for the permission key and permision check -->
      <xsd:attribute name="entity" type="Entity" use="required"/>
      <xsd:attribute name="noPermissionDisplayKey" type="xsd:string"/>
      <!-- The permission key -->
      <xsd:attribute name="permKey" type="xsd:NMTOKEN" use="required"/>
    </xsd:complexType>
  </xsd:element>
  
  <xsd:simpleType name="SystemPerm">
    <xsd:restriction base="xsd:string">
      <!-- Permission to create a new contact in the address book -->
      <xsd:enumeration value="abcreate"/>
      <!-- Permission to add a preferred vendor to the address book (Retired) -->
      <xsd:enumeration value="abcreatepref"/>
      <!-- Permission to delete an existing contact in the address book -->
      <xsd:enumeration value="abdelete"/>
      <!-- Permission to delete an existing preferred vendor address book entry -->
      <xsd:enumeration value="abdeletepref"/>
      <!-- Permission to edit an existing contact in the address book -->
      <xsd:enumeration value="abedit"/>
      <!-- Permission to modify an existing preferred vendor address book entry (Retired) -->
      <xsd:enumeration value="abeditpref"/>
      <!-- Permission to view the details of contact entries in the address book -->
      <xsd:enumeration value="abview"/>
      <!-- Permission to search contact entries in the address book -->
      <xsd:enumeration value="abviewsearch"/>
      <!-- Permission to view account file billing page -->
      <xsd:enumeration value="accountbilling"/>
      <!-- Permission to view account file claims page -->
      <xsd:enumeration value="accountclaims"/>
      <!-- Permission to view account file contacts page -->
      <xsd:enumeration value="accountcontacts"/>
      <!-- Permission to create an account -->
      <xsd:enumeration value="accountcreate"/>
      <!-- Permission to view account file documents page -->
      <xsd:enumeration value="accountdocs"/>
      <!-- Permission to view account file history page -->
      <xsd:enumeration value="accounthistory"/>
      <!-- Permission to move policies between accounts -->
      <xsd:enumeration value="accountmovepolicies"/>
      <!-- Permission to view account file notes page -->
      <xsd:enumeration value="accountnotes"/>
      <!-- Permission to view account file premiums losses page (Retired) -->
      <xsd:enumeration value="accountpremloss"/>
      <!-- Permission to view account file related accounts page -->
      <xsd:enumeration value="accountrelations"/>
      <!-- Permission to reopen a withdrawn account -->
      <xsd:enumeration value="accountreopen"/>
      <!-- Permission to rewrite policies between accounts -->
      <xsd:enumeration value="accountrewritepolicies"/>
      <!-- Permission to view account file roles page -->
      <xsd:enumeration value="accountroles"/>
      <!-- Permission to view account file summary page -->
      <xsd:enumeration value="accountsummary"/>
      <!-- Permission to withdraw an unused account -->
      <xsd:enumeration value="accountwithdraw"/>
      <!-- Permission to view account file work orders page -->
      <xsd:enumeration value="accountworkorders"/>
      <!-- Permission to approve any approval activity even if the activity is assigned to someone
           else; the approver is still subject to authority limit restrictions -->
      <xsd:enumeration value="actapproveany"/>
      <!-- Permission to create new activities -->
      <xsd:enumeration value="actcreate"/>
      <!-- Permission to modify (edit/skip/close) activities owned by other users -->
      <xsd:enumeration value="acteditunowned"/>
      <!-- Permission to set whether an activity is mandatory -->
      <xsd:enumeration value="actmakemand"/>
      <!-- Permission to own an activity.  Note that the user must be able to see the account
           or job, to own a specific activity. -->
      <xsd:enumeration value="actown"/>
      <!-- Permission to create new activity patterns -->
      <xsd:enumeration value="actpatcreate"/>
      <!-- Permission to delete activity patterns -->
      <xsd:enumeration value="actpatdelete"/>
      <!-- Permission to edit activity patterns -->
      <xsd:enumeration value="actpatedit"/>
      <!-- Permission to view the list of activity patterns or activity pattern details -->
      <xsd:enumeration value="actpatview"/>
      <!-- Permission to approve policy activity -->
      <xsd:enumeration value="actpolicyapprove"/>
      <!-- Permission to approve any policy activity -->
      <xsd:enumeration value="actpolicyapproveany"/>
      <!-- Permission to assign an activity from a queue -->
      <xsd:enumeration value="actqueueassign"/>
      <!-- Permission to get the next activity off of a queue -->
      <xsd:enumeration value="actqueuenext"/>
      <!-- Permission to pick an activity from a queue -->
      <xsd:enumeration value="actqueuepick"/>
      <!-- Permission to reassign your own activities -->
      <xsd:enumeration value="actraown"/>
      <!-- Permission to reassign activities owned by other users -->
      <xsd:enumeration value="actraunown"/>
      <!-- Permission to review and approve manually-approved assignables (Retired) -->
      <xsd:enumeration value="actreviewassign"/>
      <!-- Permission to view activities -->
      <xsd:enumeration value="actview"/>
      <!-- Permission to view all activity queues, even those in other security zones -->
      <xsd:enumeration value="actviewallqueues"/>
      <!-- Permission to execute the data change. -->
      <xsd:enumeration value="admindatachangeexec"/>
      <!-- Permission to view the data change page. -->
      <xsd:enumeration value="admindatachangeview"/>
      <!-- Administer Jobs in an Error State -->
      <xsd:enumeration value="adminerrorstatejobs"/>
      <!-- Permission to advance audits -->
      <xsd:enumeration value="advanceaudit"/>
      <!-- Permission to advance a cancellation -->
      <xsd:enumeration value="advancecancellation"/>
      <!-- Permission to advance an issuance -->
      <xsd:enumeration value="advanceissuance"/>
      <!-- Permission to advance a policy change -->
      <xsd:enumeration value="advancepolchange"/>
      <!-- Permission to advance a reinstatement -->
      <xsd:enumeration value="advancereinstate"/>
      <!-- Permission to advance a renewal -->
      <xsd:enumeration value="advancerenewal"/>
      <!-- Permission to advance rewrite -->
      <xsd:enumeration value="advancerewrite"/>
      <!-- Permission to advance rewrite new account -->
      <xsd:enumeration value="advancerewrnewacct"/>
      <!-- Permission to advance a submission -->
      <xsd:enumeration value="advancesubmission"/>
      <!-- Permission to administer Affinity Groups -->
      <xsd:enumeration value="affinitygroupadmin"/>
      <!-- Permission to view authority limit profiles (Retired) -->
      <xsd:enumeration value="alpview"/>
      <!-- Permission to create a new contact regardless of which tag(s) it has -->
      <xsd:enumeration value="anytagcreate"/>
      <!-- Permission to delete a contact regardless of which tag(s) it has -->
      <xsd:enumeration value="anytagdelete"/>
      <!-- Permission to edit the details of a contact regardless of which tag(s) it has -->
      <xsd:enumeration value="anytagedit"/>
      <!-- Permission to view the details of a contact regardless of which tag(s) it has -->
      <xsd:enumeration value="anytagview"/>
      <!-- Permission to archive objects in the database (Retired) -->
      <xsd:enumeration value="archive"/>
      <!-- Permission to disable archiving -->
      <xsd:enumeration value="archivedisable"/>
      <!-- Permission to enable archiving -->
      <xsd:enumeration value="archiveenable"/>
      <!-- Permission to create, edit, or delete user attributes -->
      <xsd:enumeration value="attrmanage"/>
      <!-- Permission to view the list of user attributes or attribute details -->
      <xsd:enumeration value="attrview"/>
      <!-- Permission to edit authority profile assignment to users -->
      <xsd:enumeration value="authprofileassignedit"/>
      <!-- Permission to view authority profile assignment to users -->
      <xsd:enumeration value="authprofileassignview"/>
      <!-- Permission to create or clone an authority profile -->
      <xsd:enumeration value="authprofilecreate"/>
      <!-- Permission to delete an authority profile -->
      <xsd:enumeration value="authprofiledelete"/>
      <!-- Permission to edit an authority profile -->
      <xsd:enumeration value="authprofileedit"/>
      <!-- Permission to view an authority profile -->
      <xsd:enumeration value="authprofileview"/>
      <!-- Permission to bind a cancellation -->
      <xsd:enumeration value="bindcancellation"/>
      <!-- Permission to bind issuance -->
      <xsd:enumeration value="bindissuance"/>
      <!-- Permission to bind policy changes -->
      <xsd:enumeration value="bindpolchange"/>
      <!-- Permission to bind reinstatement -->
      <xsd:enumeration value="bindreinstate"/>
      <!-- Permission to bind renewals -->
      <xsd:enumeration value="bindrenewal"/>
      <!-- Permission to bind a rewrite -->
      <xsd:enumeration value="bindrewrite"/>
      <!-- Permission to bind a rewrite new account -->
      <xsd:enumeration value="bindrewrnewacct"/>
      <!-- Permission to bind a submission -->
      <xsd:enumeration value="bindsubmission"/>
      <!-- Permission to rate policies in bulk for rating impact analysis (Retired) -->
      <xsd:enumeration value="bulkpolicyratingtest"/>
      <!-- Permission to create, edit, or delete business week -->
      <xsd:enumeration value="buswkmanage"/>
      <!-- Permission to view the list of business week -->
      <xsd:enumeration value="buswkview"/>
      <!-- Permission to be able to set the carrier as the source -->
      <xsd:enumeration value="cancelcarriersource"/>
      <!-- Permission to set effective date of cancellation to less than calculated date -->
      <xsd:enumeration value="cancelovereffdate"/>
      <!-- Permission to override the default refund cancellation method -->
      <xsd:enumeration value="canceloverrefund"/>
      <!-- Permission to reschedule a cancellation -->
      <xsd:enumeration value="cancelreschedule"/>
      <!-- Permission to rescind a cancellation and make other modifications during the waiting
           period -->
      <xsd:enumeration value="cancelrescind"/>
      <!-- Permission to view the cancellation wizard documents page (Retired) -->
      <xsd:enumeration value="cancwizdocs"/>
      <!-- Permission to view the cancellation history page (Retired) -->
      <xsd:enumeration value="cancwizhist"/>
      <!-- Permission to view the cancellation wizard notes page (Retired) -->
      <xsd:enumeration value="cancwiznotes"/>
      <!-- Permission to view the cancellation wizard prior history page (Retired) -->
      <xsd:enumeration value="cancwizpriorhist"/>
      <!-- Permission to view the cancellation wizard rating inputs page (Retired) -->
      <xsd:enumeration value="cancwizrateinputs"/>
      <!-- Permission to view the cancellation wizard risk analysis page (Retired) -->
      <xsd:enumeration value="cancwizrisk"/>
      <!-- Permission to view the cancellation wizard risk history page (Retired) -->
      <xsd:enumeration value="cancwizriskhist"/>
      <!-- Permission to view the cancellation wizard job roles page (Retired) -->
      <xsd:enumeration value="cancwizroles"/>
      <!-- Permission to view the cancellation wizard workplan page (Retired) -->
      <xsd:enumeration value="cancwizworkplan"/>
      <!-- Permission to change contact subtype -->
      <xsd:enumeration value="changecontactsubtype"/>
      <!-- Permission to complete audits -->
      <xsd:enumeration value="completeaudit"/>
      <!-- Permission to view contact file claims page -->
      <xsd:enumeration value="contactclaims"/>
      <!-- Permission to copy entities from one Policy to another -->
      <xsd:enumeration value="copypolicydata"/>
      <!-- Permission to create an ad-hoc audit -->
      <xsd:enumeration value="createaudit"/>
      <!-- Permission to cancel a policy -->
      <xsd:enumeration value="createcancellation"/>
      <!-- Permission to create an UW Issue -->
      <xsd:enumeration value="createevalissue"/>
      <!-- Permission to create an internal note -->
      <xsd:enumeration value="createintnote"/>
      <!-- Permission to create a new issuance -->
      <xsd:enumeration value="createissuance"/>
      <!-- Permission to create manual uw issues -->
      <xsd:enumeration value="createmanualuwissue"/>
      <!-- Permission to create a midterm change -->
      <xsd:enumeration value="createpolchange"/>
      <!-- Create referral reason -->
      <xsd:enumeration value="createreferralreason"/>
      <!-- Permission to reinstate a policy -->
      <xsd:enumeration value="createreinstate"/>
      <!-- Permission to renew a policy -->
      <xsd:enumeration value="createrenewal"/>
      <!-- Permission to create rewrite -->
      <xsd:enumeration value="createrewrite"/>
      <!-- Permission to create rewrite new account -->
      <xsd:enumeration value="createrewrnewacct"/>
      <!-- Permission to create a sensitive note -->
      <xsd:enumeration value="createsensnote"/>
      <!-- Permission to create a new submission -->
      <xsd:enumeration value="createsubmission"/>
      <!-- Permission to create a new local contact -->
      <xsd:enumeration value="ctccreate"/>
      <!-- Permission to edit an existing local contact -->
      <xsd:enumeration value="ctcedit"/>
      <!-- Permission to view and search local contact entries -->
      <xsd:enumeration value="ctcview"/>
      <!-- Permission to access debug tools, even when they are disabled by a configuration parameter -->
      <xsd:enumeration value="debugtools"/>
      <!-- Permission to decline a policy change (Retired) -->
      <xsd:enumeration value="declinepolchange"/>
      <!-- Permission to decline a submission -->
      <xsd:enumeration value="declinesubmission"/>
      <!-- Permission to delete an internal doc -->
      <xsd:enumeration value="delintdoc"/>
      <!-- Permission to delete an internal note -->
      <xsd:enumeration value="delintnote"/>
      <!-- Permission to delete a sensitive doc -->
      <xsd:enumeration value="delsensdoc"/>
      <!-- Permission to delete a sensitive note -->
