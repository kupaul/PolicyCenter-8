<?xml version="1.0"?>
<PCF
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="../../../../../pcf.xsd">
  <DetailViewPanel
    editable="true"
    id="TwilioDetailsDV">
    <Require
      name="user"
      type="User"/>
    <InputColumn>
      <Input
        editable="true"
        id="TwilioPhoneNumber"
        label="&quot;Twilio Phone Number&quot;"
        required="true"
        value="user.TwilioPhoneNumber"/>
      <TextInput
        editable="true"
        id="AccountSid"
        label="&quot;Account SID&quot;"
        required="true"
        value="user.AccountSID"/>
      <PasswordInput
        editable="true"
        id="TwilioAuthToken"
        label="&quot;Auth Token&quot;"
        required="true"
        value="user.AuthTOKEN"/>
    </InputColumn>
  </DetailViewPanel>
</PCF>